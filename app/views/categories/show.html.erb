         <% @category.products.all.each do|product|%>
          <div class="row", id="productList">
            <div class="col-lg-4 col-md-6 mb-4">
              <div class="card h-100">
                <div class="card-body">
                  <h4 class="card-title">
                    <%= link_to product.name, product_path(product.id)%></h4>
                    <%= image_tag(product.feature_image_url, size: "150x125")%>
                  <p class="card-text"><%= product.description %></p>
                </div>
              </div>
            </div>
          </div>
          <%end%>
<%= link_to "Categories list",categories_path %></br>
</br>
<p><% if user_signed_in? %><%= link_to "edit", edit_category_path(@category.id) %> | <%= link_to "delete", category_path(@category.id), method: :delete, data: {confirm: "Are you sure? "}%> <% end %></p>